		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<!-- Navigation Rules -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>descricaoProjeto</from-outcome>
			<to-view-id>/descricao_projeto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>artefatos</from-outcome>
			<to-view-id>/artefatos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>criterioAceitacao</from-outcome>
			<to-view-id>/criterios_aceitacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>escolhaProjeto</from-outcome>
			<to-view-id>/escolha_projeto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>ferramentas</from-outcome>
			<to-view-id>/ferramentas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>itensTeste</from-outcome>
			<to-view-id>/itens_teste.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>niveisTeste</from-outcome>
			<to-view-id>/niveis_teste.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>tiposTeste</from-outcome>
			<to-view-id>/tipos_teste.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>objetivo</from-outcome>
			<to-view-id>/objetivo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>pontuacao</from-outcome>
			<to-view-id>/tutorial_pontuacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>fases</from-outcome>
			<to-view-id>/tutorial_fases.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>ajuda</from-outcome>
			<to-view-id>/tutorial_ajuda.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>resultado</from-outcome>
			<to-view-id>/resultado.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>objetivoFaseProjeto</from-outcome>
			<to-view-id>/objetivo_fase_projeto.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faseProjeto</from-outcome>
			<to-view-id>/fase_projeto.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>next</from-outcome>
			<to-view-id>/fase_projeto.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>resultado_fase_projeto</from-outcome>
			<to-view-id>/resultado_fase_projeto.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>falhou</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<application>
		<resource-bundle>
			<base-name>br.ufc.si.itest.resources.mensagens</base-name>
			<var>msgn</var>
		</resource-bundle>
	</application>
	
	
</faces-config>
